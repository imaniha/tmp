{
  "name": "backend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/backend",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv run uvicorn main:app --reload --host 0.0.0.0 --port 8000",
        "cwd": "apps/backend"
      }
    },
    "sync": {
      "executor": "nx:run-commands",
      "options": {
        "command": "uv sync",
        "cwd": "apps/backend"
      }
    }
  }
}

